{
    "name": "jaeger/querylist",
    "description": "Modern, Type-Safe, Attribute-Driven PHP Web Scraper (V5).",
    "keywords": ["scraper", "crawler", "spider", "dom", "attribute", "dto"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jaeger",
            "email": "JaegerCode@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dom-crawler": "^7.0",
        "symfony/css-selector": "^7.0",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "QueryList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
